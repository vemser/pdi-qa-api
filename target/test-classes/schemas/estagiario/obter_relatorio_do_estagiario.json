{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "nomeEstagiario": {
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    },
    "nomeTrilha": {
      "type": "string"
    },
    "nomePrograma": {
      "type": "string"
    },
    "modulos": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "nome": {
              "type": "string"
            },
            "descricao": {
              "type": "string"
            },
            "status": {
              "type": "string"
            },
            "ativo": {
              "type": "boolean"
            },
            "idTrilha": {
              "type": "null"
            },
            "feedback": {
              "type": "array",
              "items": {}
            }
          },
          "required": [
            "id",
            "nome",
            "descricao",
            "status",
            "ativo",
            "idTrilha",
            "feedback"
          ]
        }
      ]
    },
    "feedbacks": {
      "type": "array",
      "items": {}
    }
  },
  "required": [
    "nomeEstagiario",
    "linkedin",
    "nomeTrilha",
    "nomePrograma",
    "modulos",
    "feedbacks"
  ]
}